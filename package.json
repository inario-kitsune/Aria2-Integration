{
  "name": "arc-integration",
  "version": "0.6.0",
  "description": "ArcIntegration - Browser extension to integrate with Aria2",
  "type": "module",
  "scripts": {
    "build": "pnpm build:webui && node scripts/build.js",
    "build:minify": "pnpm build:webui && node scripts/build.js --minify",
    "build:dev": "pnpm build:webui && node scripts/build.js --sourcemap",
    "build:webui": "cd webui && pnpm install && pnpm run build",
    "watch": "node scripts/build.js --watch",
    "package": "pnpm build:minify && cd dist && zip -r ../arc-integration.zip . -x '*.DS_Store'",
    "package:source": "node scripts/package-source.js",
    "package:all": "pnpm package && pnpm package:source",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules webui/node_modules webui/dist *.zip"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:inario-kitsune/Aria2-Integration.git"
  },
  "homepage": "https://github.com/inario-kitsune/Aria2-Integration",
  "bugs": {
    "url": "https://github.com/inario-kitsune/Aria2-Integration/issues"
  },
  "keywords": [
    "aria2",
    "firefox",
    "extension",
    "download-manager"
  ],
  "author": "RossWang",
  "license": "MIT",
  "packageManager": "pnpm@10.22.0",
  "devDependencies": {
    "esbuild": "^0.27.0"
  },
  "dependencies": {
    "jschardet": "^3.1.4"
  }
}
